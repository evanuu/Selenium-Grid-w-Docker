<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<!--parallel keyword with tests" in suite so they run parallel-->
<suite name="testsuite" parallel="tests">
    <test name="firefox test">
        <parameter name="browser" value="firefox"/>
        <parameter name="platform" value="linux"/>
        <parameter name="url" value="https://www.archdaily.com/"/>
        <classes>
            <class name="SeleniumGrid.SkyHTestFirefox"/>
        </classes>
    </test>
    <test name="chrome test">
        <parameter name="browser" value="chrome"/>
        <parameter name="platform" value="linux"/>
        <parameter name="url" value="https://www.archdaily.com/"/>
        <classes>
            <class name="SeleniumGrid.SkyHTestChrome"/>
        </classes>
    </test>
